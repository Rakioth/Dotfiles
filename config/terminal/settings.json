{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions": [
        {
            "command": "closePane",
            "keys": "ctrl+w"
        },
        {
            "command": {
                "action": "splitPane",
                "split": "auto",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+d"
        },
        {
            "command": "find",
            "keys": "ctrl+f"
        },
        {
            "command": "selectAll",
            "keys": "ctrl+a"
        },
        {
            "command": "toggleFocusMode",
            "keys": "f2"
        },
        {
            "command": {
                "action": "newTab"
            },
            "keys": "ctrl+t"
        },
        {
            "command": {
                "action": "copy",
                "singleLine": false
            },
            "keys": "ctrl+c"
        },
        {
            "command": "paste",
            "keys": "ctrl+v"
        }
    ],
    "centerOnLaunch": true,
    "copyFormatting": "none",
    "copyOnSelect": false,
    "defaultProfile": "{574e775e-4f2a-5b96-ac1e-a2962a402336}",
    "launchMode": "focus",
    "profiles": {
        "defaults": {
            "bellStyle": "none",
            "closeOnExit": "always",
            "colorScheme": "Codely Purple",
            "cursorShape": "bar",
            "experimental.retroTerminalEffect": false,
            "font": {
                "face": "CaskaydiaCove Nerd Font Mono"
            },
            "opacity": 0,
            "scrollbarState": "hidden",
            "suppressApplicationTitle": true,
            "useAcrylic": true
        },
        "list": [
            {
                "commandline": "%PROGRAMFILES%\\PowerShell\\7\\pwsh.exe -NoLogo",
                "guid": "{574e775e-4f2a-5b96-ac1e-a2962a402336}",
